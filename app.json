        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "output": "static"
    },
    "extra": {
      "eas": {
        "projectId": "926cb8db-7095-4aab-b4a0-8a9e74f61d52"
      }
    },
    "owner": "trust2025"
  }
